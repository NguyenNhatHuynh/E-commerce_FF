script.
  layout_rtl_change('false');
script.
  preset_change("preset-1");
#offcanvas_pc_layout.offcanvas.border-0.pct-offcanvas.offcanvas-end(tabindex='-1')
  .offcanvas-header.justify-content-between
    h5.offcanvas-title Settings
    button.btn.btn-icon.btn-link-danger(type='button' data-bs-dismiss='offcanvas' aria-label='Close')
      i.ti.ti-x
  .pct-body.customizer-body
    .offcanvas-body.py-0
      ul.list-group.list-group-flush
        li.list-group-item
          .pc-dark
            h6.mb-1 Theme Mode
            p.text-muted.text-sm Choose light or dark mode or Auto
            .row.theme-color.theme-layout
              .col-4
                .d-grid
                  button.preset-btn.btn.active(data-value='true' onclick="layout_change('light');")
                    span.btn-label Light
                    span.pc-lay-icon
                      span
                      span
                      span
                      span
              .col-4
                .d-grid
                  button.preset-btn.btn(data-value='false' onclick="layout_change('dark');")
                    span.btn-label Dark
                    span.pc-lay-icon
                      span
                      span
                      span
                      span
              .col-4
                .d-grid
                  button.preset-btn.btn(data-value='default' onclick='layout_change_default();' data-bs-toggle='tooltip' title="Automatically sets the theme based on user's operating system's color scheme.")
                    span.btn-label Default
                    span.pc-lay-icon.d-flex.align-items-center.justify-content-center
                      i.ph-duotone.ph-cpu
        li.list-group-item
          h6.mb-1 Sidebar Theme
          p.text-muted.text-sm Choose Sidebar Theme
          .row.theme-color.theme-sidebar-color
            .col-6
              .d-grid
                button.preset-btn.btn(data-value='true' onclick="layout_sidebar_change('dark');")
                  span.btn-label Dark
                  span.pc-lay-icon
                    span
                    span
                    span
                    span
            .col-6
              .d-grid
                button.preset-btn.btn.active(data-value='false' onclick="layout_sidebar_change('light');")
                  span.btn-label Light
                  span.pc-lay-icon
                    span
                    span
                    span
                    span
        li.list-group-item
          h6.mb-1 Accent color
          p.text-muted.text-sm Choose your primary theme color
          .theme-color.preset-color
            a.active(href='#!' data-value='preset-1')
              i.ti.ti-check
            a(href='#!' data-value='preset-2')
              i.ti.ti-check
            a(href='#!' data-value='preset-3')
              i.ti.ti-check
            a(href='#!' data-value='preset-4')
              i.ti.ti-check
            a(href='#!' data-value='preset-5')
              i.ti.ti-check
            a(href='#!' data-value='preset-6')
              i.ti.ti-check
            a(href='#!' data-value='preset-7')
              i.ti.ti-check
            a(href='#!' data-value='preset-8')
              i.ti.ti-check
            a(href='#!' data-value='preset-9')
              i.ti.ti-check
            a(href='#!' data-value='preset-10')
              i.ti.ti-check
        li.list-group-item
          h6.mb-1 Sidebar Caption
          p.text-muted.text-sm Sidebar Caption Hide/Show
          .row.theme-color.theme-nav-caption
            .col-6
              .d-grid
                button.preset-btn.btn.active(data-value='true' onclick="layout_caption_change('true');")
                  span.btn-label Caption Show
                  span.pc-lay-icon
                    span
                    span
                    span
                      span
                      span
                    span
            .col-6
              .d-grid
                button.preset-btn.btn(data-value='false' onclick="layout_caption_change('false');")
                  span.btn-label Caption Hide
                  span.pc-lay-icon
                    span
                    span
                    span
                      span
                      span
                    span
        li.list-group-item
