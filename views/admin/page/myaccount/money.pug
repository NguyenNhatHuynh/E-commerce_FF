                .d-grid
                  button.preset-btn.btn.active(data-value='false' onclick="layout_rtl_change('false');")
                    span.btn-label LTR
                    span.pc-lay-icon
                      span
                      span
                      span
                      span
              .col-6
                .d-grid
                  button.preset-btn.btn(data-value='true' onclick="layout_rtl_change('true');")
                    span.btn-label RTL
                    span.pc-lay-icon
                      span
                      span
                      span
                      span
        li.list-group-item.pc-box-width
          .pc-container-width
