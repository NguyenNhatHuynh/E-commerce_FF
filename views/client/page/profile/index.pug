       .tab-content.card-body
                      #finish.tab-pane(style="display : block;")
                        .card.table-card.border.shadow-none
                          .card-header.pb-0.pt-2
                            ul#myTab.nav.nav-tabs.profile-tabs(role='tablist')
                              li.nav-item(role='presentation')
                                button#home-tab.nav-link.active(data-bs-toggle='tab' data-bs-target='#home-tab-pane' type='button' role='tab' aria-controls='home-tab-pane' aria-selected='true') Thông Tin Đơn Hàng
                             
                          .card-body.pb-0
                            #myTabContent.tab-content
                              #home-tab-pane.tab-pane.fade.show.active(role='tabpanel' aria-labelledby='home-tab' tabindex='0')
                                .table-responsive
                                  table.table.table-hover.mb-0
                                    thead
                                      tr(style="text-align : center;")
                                        th Mã Hóa Đơn
                                        Th Trạng thái
                                        th Ngày Đặt Hàng
                                        th Tổng Tiền 
                                        th Phương Thức                                       
                                        th Hành Động 
                                        th 
                                          i.material-icons-two-tone.f-18 more_vert
                                    tbody(actionstatusotheradmin)#accordion
                                      form(action="" method="POST" formhandlecancelinvoiceclient)
                                      each item in data 
                                        tr(style="text-align : center;")
                                          td #{item.id}
                                          td 
                                           if((item.orderStatus != "cancel" && item.orderStatus != "refund") || id == 6) 
                                            span.badge.text-bg-success() #{item.orderStatus}   
                                           else 
                                            span.badge(style="background : #9A90D4;") Chờ đợi xác nhận                                       
                                          td #{item.Time}
                                          td
                                            span.badge.text-bg-success #{item.total.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VNĐ'}   
                                          td(style="color : #1DE9B6;") #{item.paymentMethod}                                        
                                          td(style = "display : flex; justify-content : center" )
                                              button.avtar.avtar-xs.btn-link-secondary(viewdetailinvoiceclient=item.id)
                                                  i.ti.ti-eye.f-20(data-bs-toggle="tooltip" title="Xem Chi Tiết!")
                                              if(id == 2 || id == 3)
                                                a.avtar.avtar-xs.btn-link-secondary(handlecancelinvoiceprofileclient=`/profile/editinvoice/${item.id}/cancel`)
                                                    <i class="fa-regular fa-circle-xmark" style="font-size : 15px;" data-bs-toggle="tooltip" title="Hủy Đơn Hàng!"></i>
